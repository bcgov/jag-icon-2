<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="ICON2.Source.EReporting.ws.provider:EReporting"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="ICON2.Source.EReporting.ws.provider:EReporting">

    <xsd:element name="getMessage">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:AppointmentMessageOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getMessageResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:AppointmentMessageOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="Error">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Reason" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getLocations">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="xsd:string"/>
                <xsd:element name="UserTokenString" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getLocationsDocument">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:locationsOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="locationsOuter">
        <xsd:sequence>
            <xsd:element name="Locations" nillable="true" type="tns:locations"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="locations">
        <xsd:sequence>
            <xsd:element name="csNum" nillable="true" type="xsd:string"/>
            <xsd:element name="Location" nillable="true" type="tns:location" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="location">
        <xsd:sequence>
            <xsd:element name="locationCd" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="getLocationsResponseDocument">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:locationsOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getLocationsResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="setMessageDate">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:AppointmentMessageOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="AppointmentMessageOuter">
        <xsd:sequence>
            <xsd:element name="AppointmentMessage" nillable="true" type="tns:appointmentMessageInner"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="appointmentMessageInner">
        <xsd:sequence>
            <xsd:element name="AppointmentMessage" type="tns:appointmentMessage"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="appointmentMessage">
        <xsd:sequence>
            <xsd:element name="csNum" nillable="true" type="xsd:string"/>
            <xsd:element name="text" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="setMessageDateResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:AppointmentMessageOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="submitAnswers">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:reportOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="reportOuter">
        <xsd:sequence>
            <xsd:element name="Report" nillable="true" type="tns:reportInner"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="reportInner">
        <xsd:sequence>
            <xsd:element name="EReport" type="tns:report"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="report">
        <xsd:sequence>
            <xsd:element name="csNum" nillable="true" type="xsd:string"/>
            <xsd:element name="deviceNo" nillable="true" type="xsd:string"/>
            <xsd:element name="eventID" nillable="true" type="xsd:string"/>
            <xsd:element name="state" nillable="true" type="xsd:string"/>
            <xsd:element name="Question" nillable="true" type="tns:question" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="question">
        <xsd:sequence>
            <xsd:element name="standardQuestionID" nillable="true" type="xsd:string"/>
            <xsd:element name="standardText" nillable="true" type="xsd:string"/>
            <xsd:element name="additionalText" nillable="true" type="xsd:string"/>
            <xsd:element name="Answer" nillable="true" type="tns:answer" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="answer">
        <xsd:sequence>
            <xsd:element name="code" nillable="true" type="xsd:string"/>
            <xsd:element name="description" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="submitAnswersResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:reportOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getAppointmentEx">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:appointmentOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="getAppointment">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="xsd:string"/>
                <xsd:element name="UserTokenString" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="appointmentOuter">
        <xsd:sequence>
            <xsd:element name="Appointment" nillable="true" type="tns:appointment"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="appointment">
        <xsd:sequence>
            <xsd:element name="csNum" nillable="true" type="xsd:string"/>
            <xsd:element name="startDate" nillable="true" type="xsd:string"/>
            <xsd:element name="endDate" nillable="true" type="xsd:string"/>
            <xsd:element name="startTime" nillable="true" type="xsd:string"/>
            <xsd:element name="endTime" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="userTokenOuter">
        <xsd:sequence>
            <xsd:element name="UserToken" type="tns:userToken"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="userToken">
        <xsd:sequence>
            <xsd:element name="RemoteClientBrowserType" type="xsd:string"/>
            <xsd:element name="RemoteClientHostName" type="xsd:string"/>
            <xsd:element name="RemoteClientIPAddress" type="xsd:string"/>
            <xsd:element name="UserIdentifier" type="xsd:string"/>
            <xsd:element name="AuthoritativePartyIdentifier" type="xsd:string"/>
            <xsd:element name="BiometricsSignature" type="xsd:string"/>
            <xsd:element name="CSNumber" type="xsd:string"/>
            <xsd:element name="SiteMinderSessionID" type="xsd:string"/>
            <xsd:element name="SiteMinderTransactionID" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="getAppointmentResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:appointmentOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="recordCompleted">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:clientLogNotificationOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="clientLogNotificationOuter">
        <xsd:sequence>
            <xsd:element name="ClientLogNotification" nillable="true" type="tns:clientLogNotificationInner"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="clientLogNotificationInner">
        <xsd:sequence>
            <xsd:element name="ClientLogNotification" nillable="true" type="tns:clientLogNotification"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="clientLogNotification">
        <xsd:sequence>
            <xsd:element name="csNum" nillable="true" type="xsd:string"/>
            <xsd:element name="eventID" nillable="true" type="xsd:string"/>
            <xsd:element name="eventState" nillable="true" type="xsd:string"/>
            <xsd:element name="deviceNo" nillable="true" type="xsd:string"/>
            <xsd:element name="message" nillable="true" type="xsd:string"/>
            <xsd:element name="messageType" nillable="true" type="xsd:string"/>
            <xsd:element name="reauthTransactionNo" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="recordCompletedResponse">
        <xsd:complexType>
            <xsd:sequence>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="recordException">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:clientLogNotificationOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="recordExceptionResponse">
        <xsd:complexType>
            <xsd:sequence>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="getQuestions">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:reportOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getQuestionsResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:reportOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getStatus">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:statusOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="statusOuter">
        <xsd:sequence>
            <xsd:element name="Status" nillable="true" type="tns:statusInner"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="statusInner">
        <xsd:sequence>
            <xsd:element name="Status" nillable="true" type="tns:status"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="status">
        <xsd:sequence>
            <xsd:element name="eventId" nillable="true" type="xsd:string"/>
            <xsd:element name="csNum" nillable="true" type="xsd:string"/>
            <xsd:element name="hasNextAppointment" nillable="true" type="xsd:string"/>
            <xsd:element name="isCurrentAppointment" nillable="true" type="xsd:string"/>
            <xsd:element name="profileEnabled" nillable="true" type="xsd:string"/>
            <xsd:element name="answersSubmitted" nillable="true" type="xsd:string"/>
            <xsd:element name="answersCorrect" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="getStatusResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:statusOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getReportingCmpltInstruction">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:reportingCmpltInstructionOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="reportingCmpltInstructionOuter">
        <xsd:sequence>
            <xsd:element name="ReportingCmpltInstruction" nillable="true" type="tns:reportingCmpltInstructionInner"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="reportingCmpltInstructionInner">
        <xsd:sequence>
            <xsd:element name="ReportingCmpltInstruction" nillable="true" type="tns:reportingCmpltInstruction"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="reportingCmpltInstruction">
        <xsd:sequence>
            <xsd:element name="csNum" nillable="true" type="xsd:string"/>
            <xsd:element name="text" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="getReportingCmpltInstructionResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:reportingCmpltInstructionOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>